{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "by": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "description": {
      "type": "text"
    },
    "tag": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tag.tag"
    },
    "image_banner": {
      "type": "component",
      "repeatable": false,
      "component": "image-banner.image-banner"
    },
    "bottom_video_url": {
      "type": "string"
    },
    "main_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "bottom_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
